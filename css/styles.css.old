/*
 * General styles
 */
* { margin: 0; padding: 0; }

article, aside, details, figcaption, figure, footer, header, hgroup, menu, nav, section { display: block; }

body {min-width:1024px; border-top: 2px solid #fff;}

#back {min-width:1024px; position:absolute; min-height:100%; width:100%; z-index:-1; background: url('http://localhost/bg-sand.png') repeat-x top center rgb(245,241,232);}
